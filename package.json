{
  "name": "@iffy/app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:db:setup": "npm run dev:db:migrate && npm run db:presets && npm run dev:db:seed && npm run stripe:products",
    "dev:db:migrate": "dotenv -e .env.local -- drizzle-kit migrate",
    "dev:db:reset": "dotenv -e .env.local -- tsx db/seed/reset.ts",
    "dev:db:seed": "dotenv -e .env.local -- tsx db/seed/index.ts",
    "dev:db:generate": "dotenv -e .env.local -- drizzle-kit generate",
    "dev:db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "dev:inngest": "dotenv -e .env.local -- inngest-cli dev",
    "stripe:products": "dotenv -e .env.local -- tsx products/index.ts",
    "db:presets": "dotenv -e .env.local -- tsx presets/index.ts",
    "build": "npm run dev:db:migrate && npm run db:presets && npm run stripe:products && next build",
    "start": "next start",
    "format": "prettier --check . --ignore-path ./.gitignore --ignore-path ./.prettierignore",
    "lint": "next lint",
    "typecheck": "tsc --noEmit --incremental",
    "test": "dotenv -e .env.local -- vitest",
    "shortest": "dotenv -e .env.local -- shortest",
    "docs:openapi:generate": "tsx scripts/generate-openapi-docs.ts",
    "postinstall": "fumadocs-mdx"
  },
  "exports": {
    "./db": "./db/index.ts",
    "./db/schema": "./db/schema.ts",
    "./strategies": "./strategies/index.ts",
    "./strategies/blocklist": "./strategies/blocklist.ts",
    "./strategies/prompt": "./strategies/prompt.ts"
  },
